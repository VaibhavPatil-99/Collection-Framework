		ArrayList<String> list = new ArrayList<String>();
		
		list.add("raju");
		list.add("prathu");
		list.add("shubh");
		list.add("vp");
		
		
		System.out.println(list);
		
//		Iterator itr = list.iterator();
//		while(itr.hasNext()) {					// Iterator is used to iterate the list.
//		System.out.print(itr.next() + " ");	
